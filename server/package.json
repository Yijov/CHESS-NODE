{
  "name": "server",
  "version": "1.0.0",
  "description": "chess game server with socket io",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "clean": "rimraf dist/",
    "copy-files": "copyfiles -u 1 src/**/*.html src/**/*.css src/**/*.map src/**/*.json src/**/*.ico src/**/*.ixt dist/",
    "build": "npm run clean && tsc -p . && npm run copy-files",
    "test": "jest"
  },
  "keywords": [
    "chess",
    "game",
    "sockets",
    "multiplayer"
  ],
  "author": "Yirbett Joseph",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "helmet": "^4.6.0",
    "joi": "^17.5.0",
    "joi-password": "^2.0.3",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^4.4.0",
    "tsyringe": "^4.6.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.5",
    "copyfiles": "^2.4.1",
    "nodemon": "^2.0.15",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  }
}
